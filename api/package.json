{
  "name": "aidrom-api",
  "type": "module",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "dump": "node dumpconfig.js",
    "db:gen": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "clear:logs": "rm -rf /logs/*",
    "clear:runtime": "rm -rf /runtime/*",
    "clear:all": "npm run clear:logs && npm run clear:runtime"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "better-sse": "^0.15.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^12.0.2",
    "drizzle-orm": "^0.44.4",
    "express": "^4.19.2",
    "gigachat": "^0.0.16",
    "pg": "^8.11.5",
    "winston": "^3.15.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "drizzle-kit": "^0.31.4",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
