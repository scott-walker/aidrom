{
  "name": "ai-helper-api",
  "type": "module",
  "imports": {
    "#config/*": "./src/config/*",
    "#db/*": "./src/db/*",
    "#routes/*": "./src/routes/*",
    "#controllers/*": "./src/controllers/*",
    "#middlewares/*": "./src/middlewares/*",
    "#services/*": "./src/services/*",
    "#clients/*": "./src/clients/*",
    "#utils/*": "./src/utils/*"
  },
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "migrate": "drizzle-kit push:pg --config=drizzle.config.js",
    "gen": "drizzle-kit generate:pg --config=drizzle.config.js"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.30.0",
    "express": "^4.19.2",
    "pg": "^8.11.5",
    "winston": "^3.15.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "drizzle-kit": "^0.31.4",
    "nodemon": "^3.1.4"
  }
}
