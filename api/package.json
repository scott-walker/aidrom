{
  "name": "ai-helper-api",
  "type": "module",
  "imports": {
    "#config/*": "./src/config/*",
    "#db/*": "./src/db/*",
    "#routes/*": "./src/routes/*",
    "#controllers/*": "./src/controllers/*",
    "#middlewares/*": "./src/middlewares/*",
    "#services/*": "./src/services/*"
  },
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "migrate": "drizzle-kit push:pg --config=drizzle.config.js",
    "gen": "drizzle-kit generate:pg --config=drizzle.config.js"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.30.0",
    "express": "^4.19.2",
    "pg": "^8.11.5"
  },
  "devDependencies": {
    "drizzle-kit": "^0.20.14",
    "nodemon": "^3.1.4"
  }
}
